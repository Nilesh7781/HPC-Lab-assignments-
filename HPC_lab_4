Write your first Parallel Program, with which you should be able to print your NAME from 4 underline cores
#include <stdio.h>
#include <omp.h>

int main() {
    // Set number of threads explicitly to 4
    omp_set_num_threads(4);

    // Parallel region
    #pragma omp parallel
    {
        int tid = omp_get_thread_num(); // thread id (0 to 3)
        
        // Each thread prints one character of the name
        if (tid == 0) printf("N");
        if (tid == 1) printf("I");
        if (tid == 2) printf("L");
        if (tid == 3) printf("E");
    }
    printf("\n");  // new line after printing name
    return 0;
}
